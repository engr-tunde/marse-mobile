{
  "expo": {
    "name": "marse",
    "slug": "marse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash/marse.png",
    "scheme": "com.themrse.com",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash/splash-2.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.themrse.com",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Allow Marsee to access your camera"
      }
    },
    "android": {
      "permissions": ["android.permission.SCHEDULE_EXACT_ALARM"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "userInterfaceStyle": "automatic",
      "package": "com.themrse.com"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash/splash-2.png",
          "imageWidth": 200,
          "backgroundColor": "#000000"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": [],
          "enableGooglePay": true
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5b1e0046-359f-4929-951d-80bb2fcfe3c5"
      }
    }
  }
}
